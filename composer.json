{
    "name": "joselfonseca/laravel-api",
    "description": "Based on Laravel API starter Kit will give you most of the boilerplate that you need for creating Laravel API's",
    "keywords": [
        "framework",
        "laravel",
        "Api",
        "REST",
        "oAuth2",
        "Dingo API"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Jose Fonseca",
            "email": "jose@ditecnologia.com",
            "homepage": "https://josefonseca.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "anhskohbo/no-captcha": "^3.0",
        "cviebrock/eloquent-taggable": "~3.4",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "dingo/api": "^2.2",
        "fideloper/proxy": "~4.0",
        "hekmatinasser/verta": "^1.9",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "jeremykenedy/laravel-logger": "^2.4",
        "kalnoy/nestedset": "^4.3",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.0",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "~1.0",
        "laravolt/avatar": "^2.1",
        "nicolaslopezj/searchable": "1.*",
        "sallar/jdatetime": "dev-master",
        "spatie/laravel-activitylog": "^3.1",
        "spatie/laravel-medialibrary": "^7.5",
        "spatie/laravel-permission": "^2.19",
        "spatie/laravel-sitemap": "^5.3",
        "spatie/laravel-sluggable": "^2.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "App\\Providers\\EventServiceProvider::postAutoloadDump"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
